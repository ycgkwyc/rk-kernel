 #define rk3588
 #include "rk3588-firefly-port.dtsi"
 #include "rk3588-diff.dtsi"

 / {
	chosen: chosen {
		bootargs = "earlycon=uart8250,mmio32,0xfeb50000 console=ttyFIQ0 irqchip.gicv3_pseudo_nmi=0 root=PARTLABEL=rootfs rootfstype=ext4 ro rootwait overlayroot=device:dev=PARTLABEL=userdata,fstype=ext4,mkfs=1 coherent_pool=1m systemd.gpt_auto=0 cgroup_enable=memory swapaccount=1 libata.force=3.0G";
	};

	/* FAN */
	fan: pwm-fan {
		compatible = "pwm-fan";
		#cooling-cells = <2>;
		fan-supply = <&vcc5v0_sys>;
		pwms = <&pwm15 0 50000 1>;
	};
	reset_ctrl_4_5g: reset-ctrlt_4_5g {		/* on ext board */
		compatible = "regulator-fixed";
		regulator-name = "reset_ctrl_4_5g";
		regulator-boot-on;
		regulator-always-on;
		enable-active-high;
		gpio = <&gpio4 RK_PB3 GPIO_ACTIVE_HIGH>;
		status = "okay";
	};	
	vcc3v3_sys_ext: vcc3v3-sys-ext {		/* on ext board */
		compatible = "regulator-fixed";
		regulator-name = "vcc3v3_sys_ext";
		regulator-boot-on;
		regulator-always-on;
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		enable-active-high;
		gpio = <&gpio3 RK_PB2 GPIO_ACTIVE_HIGH>;
		vin-supply = <&vcc12v_dcin>;
		status = "okay";
	};
	vcc5v0_sys_ext: vcc5v0-sys-ext {		/* on ext board */
		compatible = "regulator-fixed";
		regulator-name = "vcc5v0_sys_ext";
		regulator-boot-on;
		regulator-always-on;
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		enable-active-high;
		gpio = <&gpio3 RK_PC0 GPIO_ACTIVE_HIGH>;
		vin-supply = <&vcc12v_dcin>;
		status = "okay";
	};
	vcc3v8_4g_ext: vcc3v8-4g-ext {			/* on ext board */
		compatible = "regulator-fixed";
		regulator-name = "vcc3v8_4g_ext";
		regulator-boot-on;
		regulator-always-on;
		regulator-min-microvolt = <3800000>;
		regulator-max-microvolt = <3800000>;
		enable-active-high;
		gpio = <&gpio3 RK_PC1 GPIO_ACTIVE_HIGH>;
		vin-supply = <&vcc12v_dcin>;
		status = "okay";
	};

	vcc_hub3_reset_ext: vcc-hub3-reset-ext {	/* on ext board */
		compatible = "regulator-fixed";
		regulator-name = "vcc_hub3_reset_ext";
		regulator-always-on;
		enable-active-high;
		status = "okay";
		gpio = <&gpio3 RK_PD0 GPIO_ACTIVE_HIGH>;  
	};
	usb3_host_ext: usb3-host-ext {	/* on ext board */
		compatible = "regulator-fixed";
		regulator-name = "usb3_host_ext";
		regulator-always-on;
		enable-active-high;
		status = "okay";
		gpio = <&gpio3 RK_PA6 GPIO_ACTIVE_HIGH>;  
	};


};

&firefly_leds{
	
	power_led_ext: power_ext {   			/* on ext board */
		label = ":power-ext"; //blue led 
		linux,default-trigger = "ir-power-ext-click";
		default-state = "on";
		gpios = <&gpio4 RK_PB6 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&led_power_ext>;
	};
	

	user_led_ext: user_ext {			/* on ext board */
		label = ":user-ext"; //red led	
		linux,default-trigger = "ir-user-ext-click";
		default-state = "off";
		gpios = <&gpio3 RK_PD5 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&led_user_ext>;
	};
};

&pwm15{
	pinctrl-0 = <&pwm15m2_pins>;
	status = "okay";
};
/* Led */
&power_led{
       gpios = <&gpio2 RK_PC3 GPIO_ACTIVE_HIGH>;
       pinctrl-names = "default";
       pinctrl-0 = <&led_power>;
};

&user_led{
       gpios = <&gpio4 RK_PC6 GPIO_ACTIVE_HIGH>;
       pinctrl-names = "default";
       pinctrl-0 = <&led_user>;
};

/* tf-card */
&sdmmc {
	status = "okay";
	vmmc-supply = <&vcc_sdcard_pwr_en>;
};

&vcc_sdcard_pwr_en{
	/delete-property/ enable-active-high; 
	enable-active-low;
	regulator-min-microvolt = <3300000>;
	regulator-max-microvolt = <3300000>;
	gpio = <&gpio4 RK_PB0 GPIO_ACTIVE_LOW>; 
	vin-supply = <&vcc_3v3_s3>;
	status = "okay";	
};


/* es8323 audio codec */
&es8388_sound{
	status = "okay";
	/delete-property/ hp-det-gpio;
	hp-det-adc-value = <4095>; //adc value,not microvolt
	/delete-property/ spk-con-gpio;
	hp-con-gpio = <&gpio2 RK_PC4 GPIO_ACTIVE_HIGH>;
	/delete-property/ pinctrl-names;
	/delete-property/ pinctrl-0;
	rockchip,cpu = <&i2s1_8ch>;
	rockchip,audio-routing =
			"Headphone", "LOUT1",
			"Headphone", "ROUT1",
			"Headphone", "Headphone Power",
			"Headphone", "Headphone Power",
			"LINPUT1", "Main Mic",
			"LINPUT2", "Main Mic",
			"RINPUT1", "Headset Mic",
			"RINPUT2", "Headset Mic";
	linein-type = <3>;

};
&es8388{
	status = "okay";
	clocks = <&cru I2S1_8CH_MCLKOUT>;
	clock-names = "mclk";
	assigned-clocks = <&cru I2S1_8CH_MCLKOUT>;
	assigned-clock-rates = <12288000>;
	pinctrl-names = "default";
	pinctrl-0 = <&i2s1m0_mclk>;
};
&i2s1_8ch{
	status = "okay";
